{
  "footer": {
    "color": {
      "text": "#ffffff",
      "background": "#141413"
    }
  },
  "header": {
    "color": {
      "text": "#ffffff",
      "background": "#141413"
    }
  },
  "pill": {
    "color": {
      "background": {
        "default": "#e8e5e1",
        "hover": "#d1cdc7"
      }
    },
    "border-radius": "12px"
  },
  "search": {
    "color": {
      "background": "#faf7f5"
    },
    "action": {
      "color": {
        "background": {
          "default": "#faf7f5",
          "hover": "#f3f0ee"
        }
      }
    }
  },
  "shadow": {
    "default": "0 2px 10px 0 rgba(20,20,19,0.20)"
  },
  "table": {
    "color": {
      "border": {
        "resize": "#777470"
      }
    }
  },
  "border": {
    "radius": {
      "default": "4px",
      "large": "8px",
      "small": "2px"
    }
  },
  "color": {
    "action": {
      "background": {
        "hover": "#faf7f5",
        "selected": "#f3f0ee",
        "selected-hover": "#e8e5e1"
      },
      "border": {
        "focus": "#cf4500",
        "focus-on-dark": "#ffffff"
      },
      "disabled": "#b1ada6",
      "disabled-button": "#b1ada6",
      "disabled-toggle": "#b1ada6",
      "disabled-text-input": "#b1ada6",
      "on-dark": {
        "active": "#b1ada6",
        "default": "#ffffff",
        "hover": "#e8e5e1"
      },
      "primary": {
        "active": "#331505",
        "default": "#cf4500",
        "hover": "#9a3a0a"
      },
      "secondary": {
        "active": "#331505",
        "default": "#cf4500",
        "hover": "#9a3a0a"
      },
      "text": {
        "on-dark": "#141413",
        "on-disabled": "#ffffff",
        "on-hover": "#141413",
        "on-selected": "#141413",
        "on-selected-hover": "#141413"
      }
    },
    "background": {
      "default": "#ffffff",
      "gray-lightest": "#f3f0ee",
      "01": "#faf7f5",
      "02": "#e8e5e1",
      "inverse": "#141413"
    },
    "border": {
      "default": {
        "on-dark": "#555250",
        "on-light": "#e8e5e1"
      }
    },
    "brand": {
      "on-primary": "#ffffff",
      "on-secondary": "#ffffff",
      "primary": "#cf4500",
      "secondary": "#cf4500"
    },
    "feedback": {
      "error": "#ee0000",
      "error-light": "#fadede",
      "information": "#ffc61e",
      "information-light": "#fff4d1",
      "success": "#038a00",
      "success-light": "#dff7df",
      "warning": "#f38b00"
    },
    "text": {
      "default": {
        "on-dark": "#ffffff",
        "on-light": "#141413"
      },
      "helper": "#555250",
      "inverse": "#ffffff",
      "placeholder": "#777470"
    }
  },
  "font": {
    "body": {
      "family": {
        "bold": "MarkOffcForMC-Medium, MarkOffcForMC, Arial, Helvetica, sans-serif",
        "default": "MarkOffcForMC, Arial, Helvetica, sans-serif",
        "narrow": "MarkForMCNrw, MarkOffcForMC, Arial, Helvetica, sans-serif"
      },
      "size": {
        "default": "0.875rem",
        "large": "1rem",
        "small": "0.75rem"
      },
      "weight": {
        "bold": "700",
        "default": "400"
      }
    },
    "family": {
      "eyebrow": "MarkOffcForMC-Bold, MarkOffcForMC, Arial, Helvetica, sans-serif",
      "heading": "MarkOffcForMC-Light, MarkOffcForMC, Arial, Helvetica, sans-serif"
    },
    "heading": {
      "size": {
        "display": {
          "01": "5rem",
          "02": "3rem",
          "03": "2rem"
        },
        "eyebrow": {
          "01": "0.75rem",
          "02": "1rem"
        },
        "level": {
          "01": "2rem",
          "02": "1.75rem",
          "03": "1.5rem",
          "04": "1.25rem",
          "05": "1.125rem",
          "06": "1rem"
        }
      }
    }
  },
  "letter": {
    "spacing": {
      "default": "normal",
      "eyebrow": "1.8px"
    }
  },
  "line": {
    "height": {
      "default": "1.5",
      "heading": "1.25",
      "single": "1"
    }
  },
  "backdrop": {
    "color": {
      "background": "rgba(20,20,19,0.7)"
    }
  },
  "button": {
    "border-radius": "20px"
  },
  "forms": {
    "color": {
      "background": {
        "default": "#ffffff"
      },
      "border": {
        "default": "#555250"
      }
    }
  },
  "grid": {
    "gutters": {
      "lg": "1rem",
      "md": "1rem",
      "sm": "0.5rem"
    },
    "margin": {
      "lg": "1rem",
      "md": "1rem",
      "sm": "0.5rem"
    }
  },
  "link": {
    "color": {
      "default": "#cf4500",
      "on-dark": "#ffffff"
    }
  },
  "scrollbar": {
    "color": {
      "background": "#b1ada6"
    }
  }
}
